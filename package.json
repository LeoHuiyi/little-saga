{
  "name": "little-saga",
  "license": "MIT",
  "description": "Just a little-saga.",
  "version": "0.1.0-SNAPSHOT",
  "scripts": {
    "start": "babel --watch src -d lib",
    "build": "babel src -d lib",
    "test":
      "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": {
    "name": "Shi Feichao",
    "email": "842351815@qq.com",
    "url": "http://shinima.pw"
  },
  "main": "lib/index.js",
  "files": ["lib", ".editorconfig", ".gitignore", "package.json", "readme.md"],
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.jsx?$",
    "moduleFileExtensions": ["js", "json", "jsx"]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.47",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^22.4.4",
    "coveralls": "^3.0.1",
    "jest": "^22.4.4",
    "mitt": "^1.1.3",
    "prettier": "1"
  }
}
